{
  "name": "team-all-in",
  "version": "1.0.0",
  "description": "website for team all in",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile-css": "node-sass ./assets/sass/main.scss -o ./assets/css/ && node-sass ./assets/sass/gallery.scss -o ./assets/css/",
    "watch-css": "node-sass -w ./assets/sass -o ./assets/css",
    "prestart": "npm run predeploy",
    "start": "concurrently --kill-others \"npm run watch-css\" \"hugo server\"",
    "predeploy": "npm run compile-css",
    "deploy": "hugo --minify",
    "preforestry-preview": "npm run predeploy",
    "forestry-preview": "hugo -D -F -E"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bulma": "^0.7.5"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "node-sass": "^4.12.0"
  }
}
